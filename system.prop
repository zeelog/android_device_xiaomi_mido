#
# system.prop for mido
#

# Audio
ro.config.media_vol_steps=25
vendor.fastrpc.disable.adsprpcd_sensorspd.daemon=1

# Camera
persist.camera.gyro.android=0
persist.camera.gyro.disable=1
persist.camera.iface.logs=0
persist.camera.imglib.logs=0
persist.camera.isp.clock.optmz=0
persist.camera.isp.debug=0
persist.camera.mct.debug=0
persist.camera.sensor.debug=0
persist.camera.stats.test=5
persist.vendor.camera.display.lmax=1280x720
persist.vendor.camera.display.umax=1920x1080
vendor.camera.lowpower.record.enable=1
vidc.enc.dcvs.extra-buff-count=2

# Charger
ro.charger.disable_init_blank=true
ro.charger.enable_suspend=true

# Console
persist.vendor.console.silent.config=1

# Data
ro.vendor.use_data_netmgrd=true
persist.data.netmgrd.qos.enable=true
persist.vendor.data.mode=concurrent

# Disable MTE Async for system server
arm64.memtag.process.system_server=off

# Dexopt
dalvik.vm.dex2oat64.enabled=true

# Display
debug.egl.hw=0
debug.enable.sglscale=1
debug.hwui.skia_atrace_enabled=false
debug.mdpcomp.logs=0
debug.renderengine.backend=skiaglthreaded
debug.sf.hw=0
debug.sf.latch_unsignaled=1
debug.sf.recomputecrop=0
dev.pm.dyn_samplingrate=1
persist.demo.hdmirotationlock=false
persist.hwc.mdpcomp.enable=true
vendor.display.enable_default_color_mode=1

# Fingerprint
persist.qfp=false

# FM
ro.fm.transmitter=false
vendor.hw.fm.init=0

# Media
av.debug.disable.pers.cache=1
media.aac_51_output_enabled=true
media.msm8956hw=0
mmp.enable.3g2=true
vendor.vidc.debug.level=0
vendor.vidc.dec.downscalar_height=1088
vendor.vidc.dec.downscalar_width=1920
vendor.vidc.disable.split.mode=1
vendor.vidc.enc.disable_bframes=1
vendor.vidc.enc.disable.pq=true
vendor.video.disable.ubwc=1

#codecs:(PARSER_)AAC AC3 AMR_NB AMR_WB ASF FLV 3G2 MP2PS MP2TS MP3 OGG QCP WAV FLAC AIFF
vendor.mm.enable.qcom_parser=522911

# Disable media transcoding
persist.sys.fuse.transcode_user_control=true
persist.sys.fuse.transcode_enabled=false

# Perf
ro.vendor.qti.sys.fw.bservice_enable=true

# Qualcomm
ro.vendor.qti.va_aosp.support=1

# Radio (IMS)
persist.dbg.volte_avail_ovr=1
persist.dbg.vt_avail_ovr=1
persist.dbg.wfc_avail_ovr=1
persist.vendor.ims.disableIMSLogs=1
persist.vendor.ims.disableADBLogs=1
persist.vendor.ims.disableDebugDataPathLogs=1
persist.vendor.ims.disableDebugLogs=1
persist.vendor.ims.disableSigHandler=1
persist.vendor.ims.disableQXDMLogs=1

# Recovery
persist.vendor.recovery_update=false

# ScalarConfig
vendor.display.disable_scaler=1

# Shutdown
ro.build.shutdown_timeout=2

# SurfaceFlinger
debug.sf.disable_client_composition_cache=1
debug.sf.early_phase_offset_ns=1500000
debug.sf.early_app_phase_offset_ns=1500000
debug.sf.early_gl_phase_offset_ns=3000000
debug.sf.early_gl_app_phase_offset_ns=15000000
debug.sf.enable_transaction_tracing=false
debug.sf.predict_hwc_composition_strategy=0
ro.surface_flinger.enable_layer_caching=true
ro.surface_flinger.force_hwc_copy_for_virtual_displays=true
ro.surface_flinger.max_frame_buffer_acquired_buffers=3
ro.surface_flinger.max_virtual_display_dimension=4096

# System
persist.sys.binary_xml=false

# Time
persist.backup.ntpServer=0.pool.ntp.org
persist.timed.enable=true

# Telephony
ril.subscription.types=NV,RUIM
ro.telephony.call_ring.multiple=false
service.qti.ims.enabled=1
telephony.lteOnCdmaDevice=1
vendor.service.qti.ims.enabled=1
#
# Set network mode to (T/L/G/W/1X/EVDO, T/L/G/W/1X/EVDO) for 7+7 mode device on DSDS mode
#
ro.telephony.default_network=22,22

# Watchdog
ro.hw_timeout_multiplier=3

# WiFi
persist.mm.sta.enable=0

# WFD
persist.hwc.enable_vds=1

# Zygote
zygote.critical_window.minute=10
