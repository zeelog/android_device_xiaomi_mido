    <ctl name="LSM2 Port" value="None" />
    <ctl name="LSM3 Port" value="None" />
    <ctl name="LSM4 Port" value="None" />
    <ctl name="LSM5 Port" value="None" />
    <ctl name="LSM6 Port" value="None" />
    <ctl name="LSM7 Port" value="None" />
